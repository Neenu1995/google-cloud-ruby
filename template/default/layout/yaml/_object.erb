  - uid: <%= @object.path %>
    children: <%=
      if !@object.children || @object.children.empty?
        "[]"
      else
        children = "\n"
        children += @object.children.map { |child|
          "      - #{child.path}"
        }.join("\n")
        children
      end

    %>
    fullName: <%= @object.path %>
    langs:
      - ruby
    module: <%= ENV["CLOUDRAD_GEM_NAME"] %>
    id: <%= @object.name %>
    summary: "<%= YAML.dump(@object.docstring.to_str).chomp.sub("--- ", "").gsub '"', "'" %>"
    type: '<%= @object.type %>'